<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/achievement"
    android:background="@drawable/background2">

    <ImageButton
        android:layout_width="500px"
        android:layout_height="150px"
        android:id="@+id/olahraga"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="116dp"
        android:src="@drawable/olahraga" />

    <ImageButton
        android:layout_width="500px"
        android:layout_height="150px"
        android:id="@+id/menuKalori"
        android:layout_below="@+id/olahraga"
        android:layout_alignLeft="@+id/olahraga"
        android:layout_alignStart="@+id/olahraga"
        android:layout_marginTop="80dp"
        android:src="@drawable/menukalori" />

    <ImageButton
        android:layout_width="200px"
        android:layout_height="100px"
        android:id="@+id/back"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/back" />

    <ImageButton
        android:layout_width="125px"
        android:layout_height="100px"
        android:id="@+id/logout"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:src="@drawable/logout" />
</RelativeLayout>
